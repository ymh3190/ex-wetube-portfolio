extends layouts/main.pug

block content 
    h1 signup 
    include partials/error.pug
    form(method="post")
        input(type="text", name="lastName" placeholder="성")
        input(type="text", name="firstName" placeholder="이름")
        input(type="email", name="email" placeholder="이메일")
        input(type="password", name="password" placeholder="비밀번호")
        input(type="password", name="confirm" placeholder="재입력")
        button(type="submit")  생성 
    a(href="/signin") 로그인하러 가기 &rarr;
    include partials/socialNet.pug
